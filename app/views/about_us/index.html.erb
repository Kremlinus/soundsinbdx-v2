<p id="notice"><%= notice %></p>

<h1>About Us</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Role</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_us.each do |about_u| %>
      <tr>
        <td><%= about_u.name %></td>
        <td><%= about_u.role %></td>
        <td><%= about_u.description %></td>
        <td><%= link_to 'Show', about_u_url(about_u.slug) %></td>
     </tr>
    <% end %>
  </tbody>
</table>
